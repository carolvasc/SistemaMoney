	<queryString language="SQL">
		<![CDATA[SELECT sistemafinanceiro.despesas.nome_despesa,
	sistemafinanceiro.despesas.valor_despesa,
	sistemafinanceiro.categorias.tipo_categoria,
	sistemafinanceiro.pagamento.forma_pagamento
FROM sistemafinanceiro.despesas
	INNER JOIN sistemafinanceiro.pagamento ON 
	 sistemafinanceiro.despesas.pagamento_id_pagamento = sistemafinanceiro.pagamento.id_pagamento 
	INNER JOIN sistemafinanceiro.categorias ON 
	 sistemafinanceiro.despesas.categoria_id_categoria = sistemafinanceiro.categorias.id_categoria 
ORDER BY sistemafinanceiro.despesas.data_despesa ASC]]>
	</queryString>
